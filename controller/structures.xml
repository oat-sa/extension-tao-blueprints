<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE structures SYSTEM "../doc/structures.dtd">
<structures>
	<structure id="taoBlueprints_main" name="Blueprints extension" level="15" group="main">
		<description />
		<sections>
			<section id="taoBlueprints_main" name="Blueprints extension" url="/taoBlueprints/Blueprints/index">
				<trees>
					<tree name="Blueprints library"
						  className="Blueprints"
						  dataUrl="/taoBlueprints/Blueprints/getOntologyData"
						  rootNode="http://www.tao.lu/Ontologies/TAOBlueprints.rdf#Blueprints"
						  selectClass="blueprints-class-properties"
						  selectInstance="blueprints-properties"
					/>
				</trees>
				<actions allowClassActions="true">
					<action id="blueprints-new" name="New blueprints" url="/taoBlueprints/Blueprints/create" context="resource" group="tree" binding="instanciate">
						<icon id="icon-item"/>
					</action>
					<action id="blueprints-class-properties" name="Properties" url="/taoBlueprints/Blueprints/editBlueprintsClass" group="content" context="class">
						<icon id="icon-edit"/>
					</action>
					<action id="blueprints-properties" name="Properties"  url="/taoBlueprints/Blueprints/editInstance" group="content" context="instance">
						<icon id="icon-edit"/>
					</action>
					<action id="blueprints-class-new" name="New class" url="/taoBlueprints/Blueprints/addSubClass" context="resource" group="tree" binding="subClass">
						<icon id="icon-folder-open"/>
					</action>
					<action id="blueprints-delete" name="Delete" url="/taoBlueprints/Blueprints/delete" context="instance" group="tree" binding="removeNode" >
						<icon id="icon-bin"/>
					</action>
					<action id="blueprints-class-delete" name="Delete" url="/taoBlueprints/Blueprints/deleteClass" context="class" group="tree" binding="removeNode" >
						<icon id="icon-bin"/>
					</action>
					<action id="blueprints-move" name="Move" url="/taoBlueprints/Blueprints/moveInstance" context="instance" group="none" binding="moveNode">
						<icon id="icon-move-item"/>
					</action>
					<action id="blueprints-duplicate" name="Duplicate" url="/taoBlueprints/Blueprints/cloneInstance" context="instance" group="tree" binding="duplicateNode">
						<icon id="icon-copy"/>
					</action>
				</actions>
			</section>
		</sections>
	</structure>
</structures>